class Floats 
{

  
  public static inline function round(v:Float) 
  {
    return Math.round(v);
  }
  
  public static inline function ceil(v:Float) 
  {
    return Math.ceil(v);
  }
  
  public static inline function floor(v:Float) 
  {
    return Math.floor(v);
  }
  
}